    "annualInterestRate = int(input(\"年利を入力してください: \")) # 年利入力\n",
    "amount = initialInvestmentAmount \n",
    "\n",
    "for i in range(10):\n",
    "    amount = amount * (1 + (annualInterestRate / 100))\n",
    "\n",
    "print(f\"10年後の金額:{amount}\")"
   ]
